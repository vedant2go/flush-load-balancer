{
  "functions": {
    "api/slack/events.js": {
      "maxDuration": 3
    },
    "api/slack/interactions.js": {
      "maxDuration": 3
    },
    "api/health.js": {
      "maxDuration": 5
    },
    "api/load-balancer.js": {
      "maxDuration": 5
    }
  },
  "rewrites": [
    {
      "source": "/slack/events",
      "destination": "/api/slack/events"
    },
    {
      "source": "/slack/interactions", 
      "destination": "/api/slack/interactions"
    },
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/load-balancer",
      "destination": "/api/load-balancer"
    }
  ]
} 